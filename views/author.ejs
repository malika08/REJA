            <div class="left_4_wrapper">
              <div class="text_wrapper">
                <p>PHP</p>
                <p>75%</p>
              </div>
              <img src="box_images/php_line.png" alt="" srcset="" />
            </div>
            <div class="left_4_wrapper">
              <div class="text_wrapper">
                <p>WordPress</p>
                <p>85%</p>
              </div>
              <img src="box_images/wordPress_line.png" alt="" srcset="" />
            </div>
          </div>
          <!--END SKILLS-->
          <div class="marginer"></div>
          <!--EXTRA SKILLS-->
          <div class="left_5">
            <h1 class="sub_headlines">Extra Skills</h1>
            <div class="oneline">
              <img src="box_images/extra_skill_icon.png" alt="" srcset="" />
              <p>Bootstrap, Materialize</p>
            </div>
            <div class="oneline">
              <img src="box_images/extra_skill_icon.png" alt="" srcset="" />
              <p>Stylus, Sass, Less</p>
            </div>
            <div class="oneline">
              <img src="box_images/extra_skill_icon.png" alt="" srcset="" />
              <p>Gulp, Webpack, Grunt</p>
            </div>
            <div class="oneline">
              <img src="box_images/extra_skill_icon.png" alt="" srcset="" />
              <p>GIT Knowledge</p>
            </div>
          </div>
          <!--END EXTRA SKILLS-->
          <div class="marginer"></div>
