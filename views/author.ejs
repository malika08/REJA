              <img src="box_images/spanish_line.png" alt="" srcset="" />
            </div>
          </div>
          <!--END LANGUAGES-->
          <div class="marginer"></div>
          <!--SKILLS-->
          <div class="left_4">
            <h1 class="sub_headlines">Skills</h1>
            <div class="left_4_wrapper">
              <div class="text_wrapper">
                <p>HTML</p>
                <p>90%</p>
              </div>
              <img src="box_images/html_line.png" alt="" srcset="" />
            </div>
            <div class="left_4_wrapper">
              <div class="text_wrapper">
                <p>CSS</p>
                <p>85%</p>
